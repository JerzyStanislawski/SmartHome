    <TextView
        android:id="@+id/textViewGabinet"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:text="Gabinet"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/buttonGabinetUp" />

    <Button
        android:id="@+id/buttonGabinetUp"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:text="↑"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textViewGabinet"
        app:layout_constraintTop_toBottomOf="@id/buttonKuchniaUp" />

    <Button
        android:id="@+id/buttonGabinetDown"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        android:text="↓"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/buttonGabinetUp"
        app:layout_constraintTop_toBottomOf="@id/buttonKuchniaDown" />


    <TextView
        android:id="@+id/textViewWiatrolap"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:text="Wiatrołap"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/buttonWiatrolapUp" />

    <Button
        android:id="@+id/buttonWiatrolapUp"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:text="↑"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textViewWiatrolap"
